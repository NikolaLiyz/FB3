<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_control"
    tools:ignore="UnusedNavigation">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.lyz.forallbot.ui.control.ControlFragment"
        android:label="移动控制"
        tools:layout="@layout/fragment_control" />

    <fragment
        android:id="@+id/navigation_graph"
        android:name="com.lyz.forallbot.ui.graph.GraphFragment"
        android:label="地图显示"
        tools:layout="@layout/fragment_graph" />

    <fragment
        android:id="@+id/navigation_agv_state"
        android:name="com.lyz.forallbot.ui.agv_state.AgvStateFragment"
        android:label="运行状态"
        tools:layout="@layout/fragment_agv_state" />
</navigation>